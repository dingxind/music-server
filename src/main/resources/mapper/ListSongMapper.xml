<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xindong.mappers.ListSongMapper" >
  <resultMap id="BaseResultMap" type="com.xindong.entities.ListSong" >
    <id column="id" property="id" jdbcType="INTEGER" />
    <result column="song_id" property="songId" jdbcType="INTEGER" />
    <result column="song_list_id" property="songListId" jdbcType="INTEGER" />
  </resultMap>

  <update id="updateListSongMsg" parameterType="com.xindong.entities.ListSong" >

    update list_song
    <set >
      <if test="songId != null" >
        song_id = #{songId,jdbcType=INTEGER},
      </if>
      <if test="songListId != null" >
        song_list_id = #{songListId,jdbcType=INTEGER},
      </if>
    </set>
    where id = #{id,jdbcType=INTEGER}
  </update>
  <update id="updateByPrimaryKey" parameterType="com.xindong.entities.ListSong" >

    update list_song
    set song_id = #{songId,jdbcType=INTEGER},
      song_list_id = #{songListId,jdbcType=INTEGER}
    where id = #{id,jdbcType=INTEGER}
  </update>
</mapper>
